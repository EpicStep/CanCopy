{
  "name": "CanCopy",
  "description": "A web extension that allow you to copy any content from website",
  "license": "MIT",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "lint": "eslint --cache"
  },
  "dependencies": {
    "@vega-ui/icons": "^1.0.1",
    "@vega-ui/react": "^1.0.1",
    "@vega-ui/utils": "^1.0.1",
    "@wxt-dev/i18n": "^0.2.3",
    "normalize.css": "^8.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.4",
    "@wxt-dev/auto-icons": "^1.0.2",
    "@wxt-dev/module-react": "^1.1.3",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.2",
    "wxt": "^0.20.0"
  },
  "packageManager": "pnpm@10.10.0"
}
